{
	"version": "0.2.0",
	"configurations": [
		{
			"name": ".NET Core Launch (console)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build",
			"program": "${workspaceRoot}/ChocoPacker.InnoSetup/bin/Debug/netstandardapp1.5/win10-x64/ChocoPacker.InnoSetup.exe",
			"args": ["${workspaceRoot}/ChocoPacker.InnoSetup/bin/Debug/netstandardapp1.5/win10-x64/ChocoPacker.InnoSetup.dll"],
			"cwd": "${workspaceRoot}",
			"stopAtEntry": false
		},
		{
			"name": ".NET Core Test",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build",
			"program": "c:\\program files\\dotnet\\dotnet.exe",
			"args": ["${workspaceRoot}/ChocoPacker.InnoSetup.Tests test"],
			"cwd": "${workspaceRoot}",
			"stopAtEntry": false
		},
		{
			"name": ".NET Core Attach",
			"type": "coreclr",
			"request": "attach",
			"processName": "<example>"
		}
	]
}